/* pages/contact/contact.wxss */
.main{
  margin: 0;
  padding: 0 30rpx;
}

.title{
  margin-top: 50rpx;
  margin-bottom: 60rpx;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 50rpx;
}

.contanct{
  display: -webkit-flex;
  margin: 20rpx 0 30rpx 0;
  line-height: 50rpx;
  font-size: 40rpx;
  letter-spacing: 4rpx;
}

.point{
  width: 10rpx;
  height: 10rpx;
  margin-top: 25rpx;
}

.tip{
  font-size: 35rpx;
  margin-left: 30rpx;
  margin-top: 6rpx;
  width: 620rpx;
}

.line{
  height: 2rpx;
  background-color: #ccc;
  margin: 70rpx 0 80rpx 0;
}

.pos{
  width: 35rpx;
  height: 35rpx;
  margin-top: 7rpx;
}

.text{
  font-size: 35rpx;
  margin-left: 30rpx;
  width: 590rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.ditu{
  width: 100%;
}

.footer{
  margin: 30rpx 0;
}
.btn{
  width: 650rpx;
  height: 100rpx;
  line-height: 100rpx;
  color: white;
  background-color: #d2ae6a;
  border-radius: 10rpx;
}